{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/css*": ["./src/app/css/*"],
      "@/components/*": ["./src/app/components/*"],
      "@/hooks/*": ["./src/app/components/hooks/*"],
      "@/config/*":["./src/config/*"],
      "@/context/*":["./src/context/*"],
      "@/layout/*":["./src/app/layout/*"],
      "@/lib/*": ["src/lib/*"],
      "@/gen/*": ["./src/gen/*"],
      "@/public/*":["./public/*"],
      "@/style/*":["./src/app/components/style/*"],
    },
    "baseUrl": "."
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/lib/utils.js"],
  "exclude": ["node_modules"]
}
